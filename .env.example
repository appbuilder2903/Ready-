# Codesphere 2.0 Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Gemini API Key (Required for core AI features)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Google OAuth Configuration (for Google Sign-In)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# GitHub OAuth Configuration (for GitHub Sign-In)
VITE_GITHUB_CLIENT_ID=your_github_client_id_here
VITE_GITHUB_CLIENT_SECRET=your_github_client_secret_here

# External AI Service Keys (Optional)
VITE_EDEN_AI_KEY=your_eden_ai_key_here
VITE_OPENAI_KEY=your_openai_key_here
VITE_OPENROUTER_KEY=your_openrouter_key_here

# Application Configuration
VITE_APP_NAME=Codesphere 2.0
VITE_APP_VERSION=2.0.0
